{
  "compileOnSave": false,
  "compilerOptions": {
    "target": "ES2022",
    "useDefineForClassFields": true,
    "lib": ["ES2022", "DOM", "DOM.Iterable"],
    "module": "preserve",
    "skipLibCheck": true,
    "allowImportingTsExtensions": true,
    "jsx": "react-jsx",
    "noEmit": true,
    "strict": true,
    "sourceMap": true,
    "moduleDetection": "force",
    "rootDir": ".",
    "baseUrl": ".",
    "types": [
      "vite/client",
      "node",
      "@nx/react/typings/cssmodule.d.ts",
      "@nx/react/typings/image.d.ts",
      "@nx/react/typings/styled-jsx.d.ts"
    ],
    "paths": {
      "@carma-appframeworks/*": [
        "_carma_monorepo/libraries/appframeworks/*/src/index.ts"
      ],
      "@carma-commons/*": ["_carma_monorepo/libraries/commons/*/src/index.ts"],
      "@carma-commons/utils/*": [
        "_carma_monorepo/libraries/commons/utils/src/lib/*/index.ts"
      ],
      "@carma-mapping/*": ["_carma_monorepo/libraries/mapping/*/src/index.ts"],
      "@carma-providers/*": [
        "_carma_monorepo/libraries/providers/*/src/index.ts"
      ],
      "@carma/types": ["_carma_monorepo/libraries/types/src/index.d.ts"],
      "@carma/cesium": [
        "_carma_monorepo/libraries/mapping/engines/cesium/api/src/index.ts"
      ],
      "@carma/leaflet": [
        "_carma_monorepo/libraries/mapping/engines/leaflet/src/index.ts"
      ],
      "@carma/geo/*": ["_carma_monorepo/libraries/commons/geo/*/src/index.ts"],
      "@carma/geo/types": [
        "_carma_monorepo/libraries/commons/geo/types/src/index.d.ts"
      ],
      "@carma/math": ["_carma_monorepo/libraries/commons/math/src/index.ts"],
      "@carma/units/helpers": [
        "_carma_monorepo/libraries/commons/units/helpers/src/index.ts"
      ],
      "@carma/units/types": [
        "_carma_monorepo/libraries/commons/units/types/src/index.d.ts"
      ],
      "@carma-mapping/engines/cesium": [
        "_carma_monorepo/libraries/mapping/engines/cesium/legacy/src/index.ts"
      ],
      "@carma-collab/wuppertal/*": ["src/*/index.ts"]
    }
  }
}
