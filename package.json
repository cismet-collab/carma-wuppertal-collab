{
  "name": "carma-wuppertal-collab-vite",
  "private": true,
  "version": "0.106.27",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . --report-unused-disable-directives",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "serve-storybook": "http-server ./storybook-static -p 6006",
    "watch-storybook-coder": "nodemon --watch src --ext js,jsx,ts,tsx --ignore storybook-static --exec \"npm run build-storybook -- --quiet --test && npm run serve-storybook\"",
    "postversion": "echo Postversionhook && rm -f .version ; rm -f .githash ; rm -f .gitdescribe; sed '/^.*\"version\":.*$/d' package.json > .package.json.without.version && echo $npm_package_version && echo $npm_package_version > .version && git rev-parse HEAD > .githash && echo $(git describe --always --tags) | sed 's/^v//' > .gitdescribe"
  },
  "dependencies": {
    "@cismet-collab/rainhazardmaps-base-texts": "^3.2.0",
    "@fortawesome/fontawesome-free": "^6.6.0",
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-brands-svg-icons": "^6.6.0",
    "@fortawesome/free-regular-svg-icons": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "antd": "^5.19.4",
    "react-bootstrap": "^1.6.8",
    "react-cismap": "^2.9.4",
    "react-dom": "18.3.1",
    "storybook": "^9.0.12",
    "tslib": "^2.8.1",
    "ua-parser-js": "^1.0.40"
  },
  "devDependencies": {
    "@nx/react": "^21.2.0",
    "@storybook/react-vite": "^9.0.12",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "http-server": "^14.1.1",
    "nodemon": "^3.1.10",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "6.3.5"
  }
}
